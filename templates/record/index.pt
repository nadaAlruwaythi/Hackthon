<div metal:use-macro="load: ../shared/_layouts.pt">
   <div metal:fill-slot="content" tal:omit-tag="True">
<div class="wrapper"  >




  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark"> تسجيل جديد  </h1>
          </div><!-- /.col -->

        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
          <div class="col-md-12">
<div class="card card-primary ">
              <div class="card-header ">
                <h3 class="card-title  w-100 ">تسجيل جديد </h3>
              </div>
              <!-- /.card-header -->
              <!-- form start -->
              <form  method="post" enctype="multipart/form-data">
                <div class="card-body">


                  <div class="form-group">
                    <label for="exampleInputFile">ملف صوتي جديد </label>
                    <div class="input-group">
                      <div class="custom-file">
                        <input type="file" name="file" class="custom-file-input" id="exampleInputFile">
                        <label class="custom-file-label" for="exampleInputFile">اختر ملف </label>
                      </div>
                      <div class="input-group-append">
                        <span class="input-group-text" id="">رفع صوت </span>
                      </div>
                    </div>
                  </div>

                </div>
                <!-- /.card-body -->

                <div class="card-footer">
                  <button type="submit" class="btn btn-primary btn-block"> إرسال </button>
                </div>
              </form>
            </div>
          </div>

      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->



</div>
   </div>

<!-- ./wrapper -->
</div>


